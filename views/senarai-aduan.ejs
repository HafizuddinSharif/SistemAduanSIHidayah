<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="/css/styles.css">
    <title>Info Aduan</title>
  </head>
  <body>

    <!-- NAVBAR -->

    <%- include('navbar'); %>

    <div class="container">

      <h1 class="header">Senarai Aduan</h1>

      <table class="senarai-no-aduan">
        <thead>
          <tr>
            <th>No. Aduan</th>
            <th>Tarikh</th>
            <th class="table-jenis-aduan">Jenis Aduan</th>
            <th>Kawasan</th>
            <th>Lokasi</th>
          </tr>
        </thead>
        <tbody>
          <% rowsOfAduan.forEach( function(row) { %>
          <tr>
            <td><%= row.No_Aduan %></td>
            <td><%= row.Tarikh_Aduan %></td>
            <td><%= row.Nama_Bidang %></td>
            <td><%= row.Nama_Kawasan %></td>
            <td><%= row.Nama_Lokasi %></td>
          </tr>
          <% }) %>
        </tbody>
      </table>

    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="/public/javascript/navbar.js"></script>

  </body>
</html>
